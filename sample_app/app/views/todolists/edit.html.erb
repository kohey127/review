<h1>編集画面</h1>
<%= form_for(@list, url: "/todolists/#{@list.id}") do |f| %>
  <%= f.label :title, "タイトル" %><br>
  <%= f.text_field :title %><br>
  <%= f.label :body, "本文" %><br>
  <%= f.text_area :body %><br>
  <%= f.label :image, "画像" %><br>
  <%= f.attachment_field :image %><br>
  <%= f.submit "更新", disable_with: '更新ing...' %>
<% end %>